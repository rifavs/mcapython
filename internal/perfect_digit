import math

def even(number):
    for i in str(number):
        if int(i) % 2 != 0:  
            return False
    return True

def perfect_digit(start, end):
    result = []
    for num in range(start, end + 1):
        square = num * num
        if square >= 1000 and even(square):
            result.append(square)
    return result

start = int(input("Enter the start of the range: "))
end = int(input("Enter the end of the range: "))


perfect_squares =perfect_digit(start, end)

print("Perfect squares with all even digits:", perfect_squares)

