list1=[]
list2=[]
size=int(input("Enter the size of list:"))

for i in range(size):
    number=int(input(f"Enter number {i+1}:"))
    if(number%2==0):
        list1.append(number)
    else:
        list2.append(number ** 3)

print("List of even numbers:", list1)
print("List of cubes of odd numbers:", list2)
